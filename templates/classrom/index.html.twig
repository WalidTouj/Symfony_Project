<table border="1">
	<tr>
		<th>ID</th>
		<th>Name</th>
		<th>Editer</th>
		<th>Supprimer</th>
		{% for c in classrooms %}
			<tr>
				<td>{{c.id}}</td>
				<td>{{c.name}}</td>
				<td>
					<a href="{{path('update_classroom',{'id':c.id} )}}">Modifier</a>
				</td>
				<td>
					<a href="{{path('remove_classroom',{'id':c.id} )}}">Supprimer</a>
				</td>
			</tr>

		{% endfor %}
	</tr>

</table>
<a href="{{path('add_classroom')}}">Ajouter classroom</a>
