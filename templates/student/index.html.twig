<table border="1">
	<tr>
		<th>ID</th>
		<th>Email</th>
		<th>Editer</th>
		<th>Supprimer</th>
		{% for s in students %}
			<tr>
				<td>{{s.id}}</td>
				<td>{{s.Email}}</td>
				<td>
					<a href="{{path('update_student',{'id':s.id} )}}">Modifier</a>
				</td>
				<td>
					<a href="{{path('remove_student',{'id':s.id} )}}">Supprimer</a>
				</td>
			</tr>

		{% endfor %}
	</tr>

</table>
<a href="{{path('add_student')}}">Ajouter student</a>
